package com.cy.hisystem.pojo;

import java.io.Serializable;

/**
 * 封装处方表信息 
 * @author 吴凯强
 */
public class MedicalRecordPojo implements Serializable{

	private static final long serialVersionUID = -8347968462945094086L;

	/** 处方表id */
	private String id;

	/** 处方创建时间*/
	private String createDatetime;

	/** 主诉*/
	private String conditionDescription;
	
	/** 初步诊断结果 */
	private String diagnosisResult;
	
	/** 药物总费用 */
	private Double drugCost;

	/** 医嘱 */
	private String medicalOrder;
	
	/** 处方 */
	private String prescription;
	
	/** 处方号 */
	private String prescriptionnum;
	
	/** 挂号id */
	private String registerId;

	/** 拿药时间 */
	private String takingDrugDateTime;

	/** 药房操作员 */
	private String takingDrugOperator;

	/** 收费时间 */
	private String tollDateTime;
	
	/** 收费操作员*/
	private String tollOperator;

	/** 取药次数 */
	private Integer takingDrugFrequency;

	/** 划价次数 */
	private Integer tollFrequency;

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getCreateDatetime() {
		return createDatetime;
	}

	public void setCreateDatetime(String createDatetime) {
		this.createDatetime = createDatetime;
	}

	public String getConditionDescription() {
		return conditionDescription;
	}

	public void setConditionDescription(String conditionDescription) {
		this.conditionDescription = conditionDescription;
	}

	public String getDiagnosisResult() {
		return diagnosisResult;
	}

	public void setDiagnosisResult(String diagnosisResult) {
		this.diagnosisResult = diagnosisResult;
	}

	public Double getDrugCost() {
		return drugCost;
	}

	public void setDrugCost(Double drugCost) {
		this.drugCost = drugCost;
	}

	public String getMedicalOrder() {
		return medicalOrder;
	}

	public void setMedicalOrder(String medicalOrder) {
		this.medicalOrder = medicalOrder;
	}

	public String getPrescription() {
		return prescription;
	}

	public void setPrescription(String prescription) {
		this.prescription = prescription;
	}

	public String getPrescriptionnum() {
		return prescriptionnum;
	}

	public void setPrescriptionnum(String prescriptionnum) {
		this.prescriptionnum = prescriptionnum;
	}

	public String getRegisterId() {
		return registerId;
	}

	public void setRegisterId(String registerId) {
		this.registerId = registerId;
	}

	public String getTakingDrugDateTime() {
		return takingDrugDateTime;
	}

	public void setTakingDrugDateTime(String takingDrugDateTime) {
		this.takingDrugDateTime = takingDrugDateTime;
	}

	public String getTakingDrugOperator() {
		return takingDrugOperator;
	}

	public void setTakingDrugOperator(String takingDrugOperator) {
		this.takingDrugOperator = takingDrugOperator;
	}

	public String getTollDateTime() {
		return tollDateTime;
	}

	public void setTollDateTime(String tollDateTime) {
		this.tollDateTime = tollDateTime;
	}

	public String getTollOperator() {
		return tollOperator;
	}

	public void setTollOperator(String tollOperator) {
		this.tollOperator = tollOperator;
	}

	public Integer getTakingDrugFrequency() {
		return takingDrugFrequency;
	}

	public void setTakingDrugFrequency(Integer takingDrugFrequency) {
		this.takingDrugFrequency = takingDrugFrequency;
	}

	public Integer getTollFrequency() {
		return tollFrequency;
	}

	public void setTollFrequency(Integer tollFrequency) {
		this.tollFrequency = tollFrequency;
	}

	@Override
	public String toString() {
		return "HisMedicalRecordEntity [id=" + id + ", createDatetime=" + createDatetime + ", conditionDescription="
				+ conditionDescription + ", diagnosisResult=" + diagnosisResult + ", drugCost=" + drugCost
				+ ", medicalOrder=" + medicalOrder + ", prescription=" + prescription + ", prescriptionnum="
				+ prescriptionnum + ", registerId=" + registerId + ", takingDrugDateTime=" + takingDrugDateTime
				+ ", takingDrugOperator=" + takingDrugOperator + ", tollDateTime=" + tollDateTime + ", tollOperator="
				+ tollOperator + ", takingDrugFrequency=" + takingDrugFrequency + ", tollFrequency=" + tollFrequency
				+ "]";
	}

}
